<h1>Log In</h1>

<ul class="errors">
  <% if (errors) { %>
    <% errors.forEach(function (error) { %>
      <li><%= error %></li>
    <% }) %>
  <% } %>
</ul>


<form class="sign-in-form" method="post">

  <label>
    Username
    <input type="text" name="user[username]" value="<%= user.get('username') %>">
  </label>

  <label>
    Password
    <input type="password" name="user[password]" value="">
  </label>

  <input type="submit" name="Submit">

</form>
