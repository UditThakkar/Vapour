<h1>Game Store</h1>

<a href="#games/new">Submit a New Game!</a>

<ul class="game-list">
  <% games.each(function (game) { %>
    <li class="game-item-list">
      <a href="#games/<%= game.get('id') %>" class="game-item-link">
        <%= game.escape('title') %>
      </a>
    </li>
  <% }) %>
</ul>
