<h1>Sign Up</h1>

<form id="" action="<%= users_url %>" method="post">

  <%= auth_token_input %>

  <label>
    Username
    <input type="text" name="user[username]" value="<%= @user.username %>">
  </label>

  <label>
    Password
    <input type="password" name="user[password]" value="">
  </label>

  <label>
    Email
    <input type="text" name="user[email]" value="<%= @user.username %>">
  </label>

  <input type="submit" name="Submit">

</form>
